{{!-- <h1>Hello world!</h1> --}}

<form action="/" method="post" enctype="multipart/form-data">
    <h3>画像をアップ</h3>
    <input type="file" name="imageFile" accept="image/*">
    <input type="submit">
</form>

<div>
    {{#each rows}}
    <p>画像が出力</p>
    <img width="300" src="{{this.imageName}}" alt="">
    <p> {{this.imageName}} </p>
    {{/each}}
</div>